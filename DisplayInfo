
class Student {
    String name;
    int age;

    void displayInfo() {
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
    }
}

public class Main1 {
    public static void main(String[] args) {
        Student s1 = new Student();
        s1.name = "Amit";
        s1.age = 19;

        s1.displayInfo();
    }
}
